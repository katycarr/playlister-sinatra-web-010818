<h1>Create a new song: </h1>
<form action="/songs" method="post">
  Name: <input type="text" name="song[name]" id="Name">
  <br></br>
  artist:
  <br>
  <input type="text" name="artist[name]" id="Artist Name">
  <br></br>
  Choose genres:
  <br>
  <% @genres.each do |genre| %>
    <input type="checkbox" name="song[genre_ids][]" value="<%= genre.id %>" id="<%= genre.name %>"><%= genre.name %></input>
  <% end %>
  <br></br>

  <input type="submit" name="submit" value="Create Song">


</form>
